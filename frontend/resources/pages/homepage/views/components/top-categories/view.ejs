<section>
    <h2 class="z-2 header-wrap relative b ttu pa-heading-panel dib b-top-right-radius-5 bg-theme-color f15 white lh-douple">
        Top Categories</h2>
    <amp-carousel
            height="260"
            layout="fixed-height"
            type="carousel"
            class="none-scrollbar slider-container z-1"
            controls
    >
            <% categories.forEach(category =>{ %>
                <% include category category  %>
            <% }) %>

    </amp-carousel>
</section>
